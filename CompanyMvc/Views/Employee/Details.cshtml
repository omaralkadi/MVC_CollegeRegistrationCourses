@model EmployeeVM
@{
    ViewData["Title"] = "Create";
}
<h1 class="pb-4">Details Employee</h1>
<div class="form-control">
    <img src="~/Files/Images/@Model.ImageName" style="width:100px ;height:100px" />

</div>
<form asp-action="Create">
    
    <partial name="partial/DeleteDetailsPartialView"></partial>
    <a asp-action="Update" asp-route-id="@Model.Id" class="btn btn-dark">Update</a>
    <a asp-action="Index" class="btn btn-dark">Back</a>
</form>